<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	version="3.0">
	
	<enterprise-beans>
	
		<session>
			<ejb-name>IUsuarioService</ejb-name>
			<business-local>br.com.lifecondominio.service.IUsuarioService</business-local>
			<ejb-class>br.com.lifecondominio.service.impl.UsuarioService</ejb-class>
			<persistence-context-ref>
				<persistence-context-ref-name>PersistenceUnit</persistence-context-ref-name>
				<persistence-unit-name>sdapi</persistence-unit-name>
				<injection-target>
					<injection-target-class>br.com.lifecondominio.service.impl.UsuarioService</injection-target-class>
					<injection-target-name>em</injection-target-name>
				</injection-target>
			</persistence-context-ref>
		</session>
		
		<session>
			<ejb-name>ICondominioService</ejb-name>
			<business-local>br.com.lifecondominio.service.ICondominioService</business-local>
			<ejb-class>br.com.lifecondominio.service.impl.CondominioService</ejb-class>
			<persistence-context-ref>
				<persistence-context-ref-name>PersistenceUnit</persistence-context-ref-name>
				<persistence-unit-name>sdapi</persistence-unit-name>
				<injection-target>
					<injection-target-class>br.com.lifecondominio.service.impl.CondominioService</injection-target-class>
					<injection-target-name>em</injection-target-name>
				</injection-target>
			</persistence-context-ref>
		</session>
		
		<session>
			<ejb-name>IAdministradoraService</ejb-name>
			<business-local>br.com.lifecondominio.service.IAdministradoraService</business-local>
			<ejb-class>br.com.lifecondominio.service.impl.AdministradoraService</ejb-class>
			<persistence-context-ref>
				<persistence-context-ref-name>PersistenceUnit</persistence-context-ref-name>
				<persistence-unit-name>sdapi</persistence-unit-name>
				<injection-target>
					<injection-target-class>br.com.lifecondominio.service.impl.AdministradoraService</injection-target-class>
					<injection-target-name>em</injection-target-name>
				</injection-target>
			</persistence-context-ref>
		</session>
		
		<session>
			<ejb-name>IOcorrenciaService</ejb-name>
			<business-local>br.com.lifecondominio.service.IOcorrenciaService</business-local>
			<ejb-class>br.com.lifecondominio.service.impl.OcorrenciaService</ejb-class>
			<persistence-context-ref>
				<persistence-context-ref-name>PersistenceUnit</persistence-context-ref-name>
				<persistence-unit-name>sdapi</persistence-unit-name>
				<injection-target>
					<injection-target-class>br.com.lifecondominio.service.impl.OcorrenciaService</injection-target-class>
					<injection-target-name>em</injection-target-name>
				</injection-target>
			</persistence-context-ref>
		</session>
		
		<session>
			<ejb-name>IPerfilService</ejb-name>
			<business-local>br.com.lifecondominio.service.IPerfilService</business-local>
			<ejb-class>br.com.lifecondominio.service.impl.PerfilService</ejb-class>
			<persistence-context-ref>
				<persistence-context-ref-name>PersistenceUnit</persistence-context-ref-name>
				<persistence-unit-name>sdapi</persistence-unit-name>
				<injection-target>
					<injection-target-class>br.com.lifecondominio.service.impl.PerfilService</injection-target-class>
					<injection-target-name>em</injection-target-name>
				</injection-target>
			</persistence-context-ref>
		</session>

	</enterprise-beans>
	
</ejb-jar>
